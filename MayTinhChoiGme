package ktra;
import java.util.Scanner;

public class MayTinhChoiGame extends MayTinh {
    private int trongLuong;

    public MayTinhChoiGame(String tenMay, int trongLuong) {
        super(tenMay);
        this.trongLuong = trongLuong;
    }

    @Override
    public void hienThiThongTin() {
        System.out.println("Ten may: " + tenMay);
        System.out.println("Trong luong: " + trongLuong);
    }
}
